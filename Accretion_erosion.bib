@ARTICLE{perry2012estimating,
  author = {Perry, CT and Edinger, EN and Kench, PS and Murphy, GN and Smithers,
	SG and Steneck, RS and Mumby, PJ},
  title = {Estimating rates of biologically driven coral reef framework production
	and erosion: a new census-based carbonate budget methodology and
